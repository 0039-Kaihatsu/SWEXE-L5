<h1>ユーザー一覧（デバッグ用）</h1>

<% @users.each do |e| %>
  <p>
    uid: <%= e.uid %><br>
    password_digest: <%= e.password_digest %><br>
    <%= link_to "削除", user_path(e),
        data: { turbo_method: :delete, confirm: "本当に削除しますか？" } %>
  </p>
<% end %>

<p><%= link_to "新規ユーザー登録", new_user_path %></p>
<p><%= link_to "トップ画面", top_main_path %></p>
